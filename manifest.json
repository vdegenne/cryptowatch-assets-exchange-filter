{
  "manifest_version": 3,
  "name": "cryptowatch-assets-exchange-filter",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://cryptowat.ch/charts/*",
        "https://trade.kraken.com/charts/*"
      ],
      "js": ["inject.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources" : [
    {
      "resources": ["entry.js", "coinmarketcap.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
